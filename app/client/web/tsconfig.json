{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "downlevelIteration": true,
    "rootDir": "."
  },
  "include": [
    "src"
  ],
  "exclude": [
    "**/__tests__",
    "**/__deprecated__",
    "out",
    "node_modules",
    "code-intel-extensions",
    "./build/**/*"
  ]
}
