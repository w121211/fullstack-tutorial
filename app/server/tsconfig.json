{
  "compilerOptions": {
    "outDir": "dist",
    "rootDirs": [
      "src",
      "prisma",
    ],
    "lib": [
      "esnext",
      "dom"
    ],
    "esModuleInterop": true,
    "strict": true
  },
  "include": [
    "src/**/*",
    "prisma/*.ts"
  ],
  "exclude": [
    "**/__tests__",
    "**/__deprecated__",
  ]
}